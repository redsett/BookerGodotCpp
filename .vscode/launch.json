{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test in Editor",
      "type": "lldb",
      "request": "launch",
      "program": "D:/Personal/Godot/Godot_v4.2-stable_win64.exe/Godot_v4.2-stable_win64_console.exe",
      "args": [
        "-e",
        "--path",
        "${workspaceFolder}/../Booker"
      ],
      "stopAtEntry": false,
      "environment": [],
      "console": "integratedTerminal"
    },
    {
      "name": "Test runtime",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "D:/Personal/Godot/Godot_v4.2-stable_win64.exe/Godot_v4.2-stable_win64_console.exe",
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/../Booker",
      "environment": [],
      "console": "internalConsole"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "d:/Personal/Godot/BookerGodotCpp/src",
      "program": "d:/Personal/Godot/BookerGodotCpp/src/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}